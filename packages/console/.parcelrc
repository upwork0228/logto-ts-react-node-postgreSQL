{
  "extends": "@parcel/config-default",
  "transformers": {
    "raw:*": ["@parcel/transformer-raw"],
    "**/assets/**/*.svg": [
      "@parcel/transformer-svg-react"
    ],
    "*.{md,mdx}": [
      "@parcel/transformer-mdx"
    ]
  },
  "compressors": {
    "*.{html,css,js,svg,map}": [
      "...",
      "@parcel/compressor-gzip",
      "@parcel/compressor-brotli"
    ]
  }
}
